{
  "name": "freemart-backend",
  "version": "0.1.0",
  "description": "FreeMart backend API (Node + Express + MySQL)",
  "main": "src/index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --watch src --exec \"node src/index.js\"",
    "start:dev": "set DB_PATH=..\\freemart.db && node src/index.js",
    "migrate": "npx sequelize db:migrate",
    "seed": "npx sequelize db:seed:all",
    "lint": "eslint .",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "bullmq": "^1.67.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "express-validator": "^7.3.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.2.0",
    "puppeteer": "^24.31.0",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.0",
    "sharp": "^0.32.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "freemart",
    "marketplace",
    "express",
    "mysql"
  ],
  "author": "",
  "license": "MIT"
}
